score              DSR_AFSLANK_BODY              1.0

meta               DSR_AFSLANKSPAM               DSR_AFSLANK_FROM + DSR_AFSLANK_SUBJ + DSR_AFSLANK_BODY >= 3
describe           DSR_AFSLANKSPAM               Afslank
score              DSR_AFSLANKSPAM               5.0

